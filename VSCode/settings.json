{
    "window.zoomLevel": 1,
    "files.autoSave": "afterDelay",
    "extensions.ignoreRecommendations": false,
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "j"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "t"
            ],
            "commands": [
                "workbench.action.toggleSidebarVisibility"
            ]
        },
        {
            "before": [
                "<leader>",
                "z"
            ],
            "commands": [
                "revealInExplorer"
            ],
        },
        {
            "before": [
                "g",
                "R"
            ],
            "commands": [
                "fileutils.renameFile"
            ]
        },
        {
            "before": [
                "]",
                "e"
            ],
            "commands": [
                "editor.action.marker.next"
            ]
        },
        {
            "before": [
                "[",
                "e"
            ],
            "commands": [
                "editor.action.marker.prev"
            ]
        },
        {
            "before": [
                "g",
                "a"
            ],
            "commands": [
                "editor.action.refactor"
            ]
        },
        {
            "before": [
                "g",
                "o"
            ],
            "commands": [
                "outline.focus"
            ]
        },
        {
            "before": [
                "=",
                "="
            ],
            "commands": [
                "editor.action.formatDocument"
            ]
        },
        {
            "before": [
                "<leader>",
                "d"
            ],
            "after": [
                "d",
                "d"
            ]
        },
        {
            "before": [
                "<C-n>"
            ],
            "commands": [
                ":nohl"
            ]
        },
        {
            "before": [
                "<leader>",
                "m"
            ],
            "commands": [
                "bookmarks.toggle"
            ]
        },
        {
            "before": [
                "<leader>",
                "b"
            ],
            "commands": [
                "bookmarks.list"
            ]
        },
        {
            "before": [
                "Z",
                "Z"
            ],
            "commands": [
                ":wq"
            ]
        },
        {
            "before": [
                "Z",
                "Q"
            ],
            "commands": [
                ":q!"
            ]
        },
        {
            "before": [
                "<leader>", "<leader>"
            ],
            "commands": [
                "workbench.action.toggleZenMode"
            ]
        }
    ],
    "vim.leader": "<space>",
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-f>": false
    },
    "vim.digraphs": {}
}
